#!/bin/bash

# Fonts
## Monospace fonts
monospace=google-inconsolata-fonts
monospace="$monospace google-roboto-mono-fonts"

## Other
other=google-roboto-fonts

# Adding a better font experience for our Linux environment
other="$other fetchmsttfonts"
other="$other fontawesome-fonts"

# Get, show and execute command
cmd="$monospace $other"
echo $cmd
